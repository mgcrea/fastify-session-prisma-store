{
  "name": "@mgcrea/fastify-session-prisma-store",
  "version": "0.3.0",
  "description": "Prisma store for fastify-session",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run spec -- --watch",
    "build": "rimraf lib/*; tsc --build",
    "lint": "eslint src/ test/",
    "prettycheck": "prettier --check src/ test/",
    "typecheck": "tsc --noEmit",
    "spec": "DEBUG=fastify-session*,prisma-query jest --runInBand",
    "test": "npm run lint && npm run prettycheck && npm run typecheck && npm run spec",
    "prepublishOnly": "npm run build"
  },
  "author": "Olivier Louvignes <olivier@mgcrea.io>",
  "repository": "github:mgcrea/fastify-session-prisma-store",
  "license": "MIT",
  "devDependencies": {
    "@mgcrea/fastify-session": "^0.14.2",
    "@tsconfig/node12": "^1.0.9",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.18",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint": "^8.9.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  },
  "peerDependencies": {
    "@prisma/client": ">=3"
  },
  "dependencies": {
    "debug": "^4.3.3"
  },
  "engines": {
    "node": ">=12"
  },
  "keywords": [
    "prisma",
    "postgresql",
    "fastify",
    "session",
    "auth"
  ]
}
